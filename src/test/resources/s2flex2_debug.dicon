<?xml version="1.0" encoding="Shift_JIS"?>
<!DOCTYPE components PUBLIC "-//SEASAR//DTD S2Container 2.3//EN" 
	"http://www.seasar.org/dtd/components23.dtd">
<components namespace="s2flex2">
	<!-- amf io -->
	<include path="amf3_debug.dicon" />
	
	<include path="aop.dicon"/>
	<component class="org.seasar.framework.container.autoregister.AspectAutoRegister">
		<property name="interceptor">aop.traceInterceptor</property>
		<initMethod name="addClassPattern">
			<arg>"org.seasar.flex2"</arg>
			<arg>".*Impl"</arg>
		</initMethod>
	</component>
	
	<!-- util -->
	<component class="org.seasar.flex2.util.data.transfer.impl.TransferImpl" />
	<component class="org.seasar.flex2.util.data.storage.impl.StorageLocatorImpl"/>
	<component name="serviceDataStorage" class="org.seasar.flex2.util.data.storage.impl.HttpSessionDataStorage" instance="session" />
	
	<!-- service -->
	<component class="org.seasar.flex2.rpc.remoting.service.impl.RemotingServiceRepositoryImpl" />
	<component class="org.seasar.flex2.rpc.remoting.service.impl.RemotingServiceLocatorImpl" />
	<component name="serviceInvoker" class="org.seasar.flex2.rpc.messaging.invoker.impl.MessagingServiceInvoker" />
	
	<!-- amf processor -->
	<component class="org.seasar.flex2.rpc.gateway.session.impl.TomcatSessionDecoratorImpl" />
	<component class="org.seasar.flex2.rpc.remoting.processor.impl.AmfHeaderProcessorImpl" />
	<component class="org.seasar.flex2.rpc.remoting.processor.impl.AmfBodyProcessorImpl">
		<initMethod>#self.addInvoker(serviceInvoker)</initMethod>
	</component>
	<component class="org.seasar.flex2.rpc.remoting.processor.impl.AmfProcessorImpl"/>
</components>